				<br><br>
				<h1>Summary</h1>
<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<th>Name</th>
			<th>Actions</th>
		</tr>
	</thead>
	<tbody>
		<% @subjects.each do |b| %>
		<tr>
			<td><%= b.subject_name %></td>
			<td>
				<%= link_to "Show", subject_path(b.id) %>
				<%= link_to "Delete", subject_path(b.id), :method => :delete %>
				<%= link_to "Edit", edit_subject_path(b.id) %>
			</td>
		</tr>
		<% end %>
	</tbody>
</table>